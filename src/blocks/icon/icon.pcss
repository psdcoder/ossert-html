.icon {
  display: inline-block;
  width: 1em;
  height: 1em;
  vertical-align: middle;
  stroke-width: 0;
  stroke: currentColor;
  fill: currentColor;

  &_inline {
    display: inline;
  }

  &_type_mark-a {
    color: $indicatorRateA;
  }

  &_type_mark-b {
    color: $indicatorRateB;
  }

  &_type_mark-c {
    color: $indicatorRateC;
  }

  &_type_mark-d {
    color: $indicatorRateD;
  }

  &_type_mark-e {
    color: $indicatorRateE;
  }
}
